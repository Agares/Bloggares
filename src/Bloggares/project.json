{
	"webroot": "wwwroot",
	"version": "1.0.0-*",
	"exclude": [
		"wwwroot"
	],
	"packExclude": [
		"**.kproj",
		"**.user",
		"**.vspscc"
	],
	"dependencies": {
		"Microsoft.AspNet.Mvc": "6.0.0-beta3",
		"Microsoft.AspNet.Server.WebListener": "1.0.0-beta3",
		"Microsoft.Framework.DependencyInjection": "1.0.0-beta3",
		"Microsoft.Framework.ConfigurationModel": "1.0.0-beta3",
		"Microsoft.Framework.ConfigurationModel.Json": "1.0.0-beta3",
		"Bloggares.Database": "",
		"Bloggares.Database.Commands": "",
		"Bloggares.Commands": "",
		"Bloggares.Core": ""
	},
	"compilationOptions": {
		"languageVersion": "experimental",
		"warningsAsErrors": true
	},
	"commands": {
		"db": "Bloggares.Database.Commands",
		"bloggeres": "Bloggares.Commands",
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5001"
	},
	"frameworks": {
		"aspnet50": {
			"frameworkAssemblies": {
				"System.Data": "4.0.0.0"
			}
		} /*,
			// Npgsql does not support CoreCLR
		"aspnetcore50": {
		}*/
	}
}